def linearsearch(array,n,x):
  for i in range(0,n):
    if (array[i]==x):
      return i
    return -1
array=[65,20,10,55,32,12,50]
n=int(input("enter a number"))
x=len(array)
result = linearsearch(array,n,x)
if (result==-1):
  print("elemnt not found")
else:
  print("element found at index: ",result) 
