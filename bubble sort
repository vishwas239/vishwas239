def bubblesort(arr):
      n=len(arr)
      swapped = False
      for i in range(n-1):
       for j in range(0,n-i-1):
         if arr[j] > arr[j+1]:
           swapped = True
         arr[j],arr[j+1]=arr[j+1],arr[j]
        
arr = [5,3,6,7,8,91,85]
bubblesort(arr)
print("the sorted array is : ")
for i in range(len(arr)):
  print("%d"%arr[i],end="")
